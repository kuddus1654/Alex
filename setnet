Q1. Calculator 
using System;
class Program
{
    static void Main()
    {
        Console.Write("Gr no:62, Name:Kuddus Shaikh\nP: ");
        double p = Convert.ToDouble(Console.ReadLine());
       Console.Write("R%: ");
        double r = Convert.ToDouble(Console.ReadLine());
        Console.Write("T: ");
        double t = Convert.ToDouble(Console.ReadLine());
        double si = p * r * t / 100;
        double ci = p * Math.Pow(1 + r / 100, t) - p;
        Console.WriteLine("\nSI = {0}\nCI = {1}", Math.Round(si, 2), Math.Round(ci, 2));
        Console.ReadKey();
    }
}


Q2. Fibonacci Series 
using System;
class P
{
    static void Main()
    {
        Console.Write("Gr no:62, Name:Kuddus Shaikh\nTerms: ");
        int n = int.Parse(Console.ReadLine()), a = 0, b = 1;
        while (n-- > 0) { Console.Write(a + " "); b += a; a = b - a; }
        Console.ReadKey();
    }
}

Q3.Boxing and unboxing 
using System;
class P
{
    static void Main()
    {
        Console.Write("GRno:62, Name:Kuddus Shaikh\n");
        int a = 10; object obj = a; int b = (int)obj;
        Console.WriteLine("a={0}\nobj={1}\nb={2}", a, obj, b);
        Console.ReadKey();
    }
}

